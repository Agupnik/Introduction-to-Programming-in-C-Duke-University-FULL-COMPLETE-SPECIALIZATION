#include <stdio.h>
#include <stdlib.h>

unsigned power(unsigned x, unsigned y);
int main()
{
  if(power(0, 0) != 1){
    return EXIT_FAILURE;
  }
  if(power(1, 0) != 1){
    return EXIT_FAILURE;
  }
  if(power(0, 3) != 0){
    return EXIT_FAILURE;
  }
  if(power(2, 4) != 16){
    return EXIT_FAILURE;
  }
  if(power(3.14, 3.8) != 27){
    return EXIT_FAILURE;
  }

  return EXIT_SUCCESS;
}
